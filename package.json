{
  "name": "ballet-submit-labextension",
  "version": "0.4.0",
  "description": "Submit ballet modules from within JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/HDI-Project/ballet-submit-labextension",
  "bugs": {
    "url": "https://github.com/HDI-Project/ballet-submit-labextension/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Micah Smith",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/HDI-Project/ballet-submit-labextension.git"
  },
  "scripts": {
    "build": "jlpm run build:lib",
    "build:labextension": "cd server/ballet-submit-labextension && rimraf labextension && mkdirp labextension && cd labextension && npm pack ../../..",
    "build:lib": "tsc",
    "build:all": "jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf server/ballet-submit-labextension/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "lint": "jlpm run tslint && jlpm run prettier",
    "prepare": "jlpm run clean && jlpm run build",
    "prettier": "prettier --write \"**/*{.ts,.tsx,.css,.json}\"",
    "tslint": "tslint --fix -c tslint.json '**/*{.ts,.tsx}'",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.1",
    "@jupyterlab/apputils": "^2.0.1",
    "@jupyterlab/docregistry": "^2.0.1",
    "@jupyterlab/notebook": "^2.0.1",
    "@jupyterlab/settingregistry": "^2.0.1",
    "@lumino/disposable": "^1.3.1",
    "axios": "^0.19.2",
    "react": "~16.9.0"
  },
  "devDependencies": {
    "@types/react": "~16.9.0",
    "rimraf": "^2.6.1",
    "prettier": "^1.19.1",
    "typescript": "~3.7.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "ballet-submit-labextension"
        }
      }
    },
    "extension": true,
    "schemaDir": "schema"
  }
}
